package hari;

 class B extends Thread {
	public void  run() {
		for(int i=0;i<=10;i++) {
			if(i==2) {
				System.out.println(i+"even number");
			}
			for(int j=0;j<=10;j++) {
				if(j!=2) {
					System.out.println(j+"odd number");
				}
			}
				
			
		}
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		A ob=new A();
		A obj=new A();
		ob.run();
		obj.run();

	}

}
