package array;

public class DoubleExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
    double[]  value= new double[10];
    
    value[0]=23.5;
    value[1]=33.33;
    value[2]=44.44;
    for (int i=0;i<value.length;i++) {
    	System.out.println(value[i]);
    }
	}

}
