package uniqtask;

public class B {
		String a;
		int b;
		void add() {
			System.out.println("go");
			a="hello";
			b=20;
		}
		void div() {
			System.out.println("have a nice day");
			a="welcome";
			b=40;
		}
		public static void main(String[] args) {
			B obj=new B();
			System.out.println(obj.);
			System.out.println(obj.);
			obj.add();
			System.out.println(obj.);
			System.out.println(obj.);
			obj.div();
			System.out.println(obj.);
			System.out.println(obj.);

	}

}







































